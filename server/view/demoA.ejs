<%- include('common/header', {title: title}); %>
<h1 style="color: #ff0000;">demo A页面</h1>
<h2>demo A页面</h2>
<div class="div2" style="margin-bottom: 30px;">
  <a href="/demoB">这是去往demoB的链接</a>
</div>
<div>
  <p>下面是用于测试demoA自定义事件的按钮</p>
  <button id="customer-button1">demoA测试事件一</button>
  <button id="customer-button2">demoA测试事件二</button>
</div>
<%- include('common/demo'); %>
<script>
  document.getElementById("customer-button1").onclick = function () {
    if (sdk) {
      sdk.dispatch('click', this, {
        type: 'demo-a-1',
        clickCount: 1,
        httpCode: 200,
        serviceCode: 200
      })
    }
  };
  document.getElementById("customer-button2").onclick = function () {
    if (sdk) {
      sdk.dispatch('click', this, {
        type: 'demo-a-2',
        clickCount: 1,
        httpCode: 200,
        serviceCode: 200
      })
    }
  };
</script>
